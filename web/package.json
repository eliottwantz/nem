{
	"name": "web",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev --host",
		"build": "prisma generate && vite build",
		"preview": "vite preview",
		"sync": "svelte-kit sync",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check .",
		"format": "prettier --write .",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate dev",
		"db:studio": "prisma studio",
		"db:seed": "prisma db seed",
		"i18n:gen": "paraglide-js compile --project ./project.inlang",
		"i18n:validate": "inlang validate --project ./project.inlang",
		"i18n:lint": "inlang lint --project ./project.inlang",
		"i18n:translate": "inlang machine translate -f --project ./project.inlang"
	},
	"devDependencies": {
		"@auth/core": "^0.18.0",
		"@auth/prisma-adapter": "^1.0.5",
		"@auth/sveltekit": "^0.3.11",
		"@aws-sdk/client-s3": "^3.445.0",
		"@event-calendar/core": "^2.3.3",
		"@event-calendar/day-grid": "^2.3.3",
		"@event-calendar/interaction": "^2.3.3",
		"@event-calendar/list": "^2.3.3",
		"@event-calendar/time-grid": "^2.3.3",
		"@faker-js/faker": "^8.3.1",
		"@fontsource-variable/inter": "^5.0.15",
		"@fontsource/gravitas-one": "^5.0.17",
		"@inlang/cli": "^2.0.0",
		"@inlang/paraglide-js-adapter-vite": "^1.0.1",
		"@prgm/sveltekit-progress-bar": "^1.0.0",
		"@skeletonlabs/skeleton": "^2.2.0",
		"@skeletonlabs/tw-plugin": "^0.2.1",
		"@sveltejs/adapter-vercel": "^3.0.3",
		"@sveltejs/enhanced-img": "^0.1.6",
		"@sveltejs/kit": "^1.25.1",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/nodemailer": "^6.4.14",
		"@videosdk.live/js-sdk": "^0.0.73",
		"@videosdk.live/rtc-js-prebuilt": "0.3.34",
		"autoprefixer": "^10.4.16",
		"emoji-picker-element": "^1.18.4",
		"nodemailer": "^6.9.7",
		"postcss": "^8.4.31",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.4",
		"prisma": "^5.6.0",
		"stripe": "^14.0.0",
		"svelte": "^4.2.1",
		"svelte-check": "^3.5.2",
		"svelte-email": "^0.0.4",
		"sveltekit-superforms": "^1.7.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.2",
		"tsx": "^4.0.0",
		"turbo": "^1.11.2",
		"typescript": "^5.2.2",
		"vite": "^4.4.9",
		"vite-plugin-kit-routes": "^0.2.1",
		"vite-plugin-tailwind-purgecss": "^0.1.3",
		"zod": "^3.22.2"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.5.3",
