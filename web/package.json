	"pnpm": {
		"patchedDependencies": {
			"@videosdk.live/rtc-js-prebuilt@0.3.34": "patches/@videosdk.live__rtc-js-prebuilt@0.3.34.patch"
		}
	},
	"prisma": {
		"seed": "tsx prisma/seed.ts"
	}
